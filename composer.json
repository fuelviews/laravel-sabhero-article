{
    "name": "fuelviews/laravel-sabhero-blog",
    "description": "This is my package laravel-sabhero-blog",
    "keywords": [
        "fuelviews",
        "laravel",
        "laravel-sabhero-blog"
    ],
    "homepage": "https://github.com/fuelviews/laravel-sabhero-blog",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Mitchener",
            "email": "support@fuelviews.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-zip": "*",
        "althinect/filament-spatie-roles-permissions": "^2.2",
        "diglactic/laravel-breadcrumbs": "^9.0",
        "embed/embed": "^4.4",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/support": "^3.2",
        "fuelviews/laravel-sabhero-wrapper": ">=0.0.13",
        "jeffgreco13/filament-breezy": "^2.5",
        "spatie/filament-markdown-editor": "^2.0",
        "spatie/laravel-package-tools": "^1.19",
        "spatie/laravel-permission": "^6.13"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "rector/rector": "^2.0",
        "driftingly/rector-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Fuelviews\\SabHeroBlog\\": "src/",
            "Fuelviews\\SabHeroBlog\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fuelviews\\SabHeroBlog\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fuelviews\\SabHeroBlog\\SabHeroBlogServiceProvider"
            ],
            "aliases": {
                "SabHeroBlog": "Fuelviews\\SabHeroBlog\\Facades\\SabHeroBlog"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
